FindAndClickListTarget(target, predicatePattern := "") {
    global patterns

    interval := 500
    maxCount := 12
    variancePct := 20
    result := SwipeUntilDetect(target, { variancePct : variancePct, interval : interval, maxCount : maxCount, callback : Func("SwipeDown") })
    directionUp := true

    while (!result.IsSuccess) {
        PollPattern([patterns.companions.refresh], { variancePct : variancePct, doClick : true, maxCount : 10 })
        sleep, 500

        if (directionUp) {
            result := SwipeUntilDetect(target, { variancePct : variancePct, interval : interval, maxCount : maxCount, callback : Func("SwipeUp") })
            directionUp := false
        }
        else {
            result := SwipeUntilDetect(target, { variancePct : variancePct, interval : interval, maxCount : maxCount, callback : Func("SwipeDown") })
            directionUp := true
        }
    }

    count := 0
    Loop {
        sleep, 500
        result := FindPattern(target, { variancePct : variancePct, doClick : true })
        count++

        if (!result.IsSuccess && mod(count, 6) = 0) {
            if (directionUp) {
                SwipeUp()
            }
            else {
                SwipeDown()
            }
        }
    } until (result.IsSuccess)
    
}

SwipeUntilDetect(pattern, opts := "") {
    opts := InitOps(opts, { interval : 500, maxCount : 100, callback : ""})

    count := 0

    result := FindPattern(pattern, opts)

    while (!result.IsSuccess && count < opts.maxCount) {
        opts.callback()
        sleep, opts.interval
        result := FindPattern(pattern, opts)
        count++
    }

    return result
}

